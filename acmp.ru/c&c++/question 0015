     #include<iostream>
     using namespace std;
     int main() { 
         int n,i,a,k=0,j;
         cin>>n;
         for(i=1;i<=n;i++) { 
             for(j=1;j<=n;j++) {
                 cin>>a;
                 if(a==1) {
                     k++;    
                 } 
                 
             } 
             
         } 
         cout<<k/2;
     }

